<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script src="https://cdn.tailwindcss.com"></script>
    <title>Document</title>
  </head>
  <body>
    <a href="/">Go back</a>
    <div class="bg-black text-white">
      <% users.forEach((user) => { %>
      <img class="h-32 w-32" src="<%=user.imagee%>" alt="" />
      <h1><%= user.name %></h1>
      <h1><%= user.email %></h1>
      <form
        action="/delete/<%= user._id %>"
        method="POST"
        style="display: inline"
      >
        <button type="submit" class="text-red-500">delete user</button>
      </form>

        <a href="/update/<%=user.id%>" class="text-white ">update user</a>
      <% }); %>
    </div>
  </body>
</html>
